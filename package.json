{
  "name": "react-beginner-kit",
  "version": "1.0.0",
  "description": "Basic react js application setup",
  "scripts": {
    "start": "npm-run-all --parallel start:dev lint:watch",
    "start:dev": "babel-node tools/run start",
    "open:src": "babel-node tools/srcServer.js",
    "test:local": "mocha --opts mocha.opts",
    "lint": "eslint src",
    "lint:changed": "LIST=`git diff-index --name-only HEAD | grep '.jsx\\{0,1\\}'$`; if [ \"$LIST\" ]; then eslint $LIST; fi",
    "lint:changed:fix": "LIST=`git diff-index --name-only HEAD | grep '.jsx\\{0,1\\}'$`; if [ \"$LIST\" ]; then eslint $LIST --fix; fi",
    "lint:watch": "esw src -w --ignore-path src/server.js",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "build:production": "NODE_ENV=production npm run build -- --release"
  },
  "precommit": {
    "run": [
      "lint:changed"
    ]
  },
